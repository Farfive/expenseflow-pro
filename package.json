{
  "name": "expenseflow-pro-backend",
  "version": "1.0.0",
  "description": "Backend API for ExpenseFlow Pro - Modern expense management platform",
  "main": "simple-server.js",
  "scripts": {
    "start": "node simple-server.js",
    "dev": "node simple-server.js",
    "dev:full": "concurrently \"node simple-server.js\" \"cd frontend && npm run dev\"",
    "dev:backend": "node simple-server.js",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:services": "echo 'Services not needed for simple mode'",
    "queue:worker": "echo 'Queue worker not needed for simple mode'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:api": "jest --testPathPattern=tests/api",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:api && npm run test:e2e",
    "test:ci": "cross-env CI=true npm run test:all",
    "test:report:allure": "allure generate allure-results --clean && allure open",
    "test:setup": "playwright install",
    "test:start-and-test": "start-server-and-test start http://localhost:3001 test:e2e",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "prisma:generate": "echo 'Prisma not needed for simple mode'",
    "prisma:push": "echo 'Prisma not needed for simple mode'",
    "prisma:seed": "echo 'Prisma not needed for simple mode'",
    "prisma:studio": "echo 'Prisma not needed for simple mode'",
    "prisma:reset": "echo 'Prisma not needed for simple mode'",
    "setup": "npm install && cd frontend && npm install",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "echo 'Backend build complete'",
    "build:frontend": "cd frontend && npm run build",
    "docker:build": "docker build -t expenseflow-pro .",
    "docker:run": "docker-compose up -d",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:stop": "docker-compose down",
    "clean": "rm -rf node_modules frontend/node_modules .next frontend/.next uploads logs",
    "reset": "npm run clean && npm run setup"
  },
  "keywords": [
    "expense",
    "management",
    "ocr",
    "document",
    "processing",
    "machine-learning",
    "categorization",
    "bank-statements",
    "transaction-processing",
    "polish",
    "business",
    "api"
  ],
  "author": "ExpenseFlow Team",
  "license": "PROPRIETARY",
