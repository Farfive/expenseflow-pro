{
  "name": "expenseflow-pro-frontend",
  "version": "1.0.0",
  "description": "Modern React/Next.js frontend for ExpenseFlow Pro - Polish expense management platform",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4000",
    "dev:optimized": "TURBO=1 NEXT_TELEMETRY_DISABLED=1 next dev -p 4000 --turbo",
    "dev:fast": "NODE_OPTIONS='--max-old-space-size=8192' next dev -p 4000 --turbo",
    "build": "next build",
    "build:optimized": "NEXT_TELEMETRY_DISABLED=1 next build",
    "start": "next start -p 4000",
    "lint": "next lint",
    "lint:fast": "next lint --dir src --dir pages --dir components",
    "type-check": "tsc --noEmit",
    "type-check:fast": "tsc --noEmit --skipLibCheck",
    "clean": "rimraf .next .swc node_modules/.cache",
    "clean:all": "rimraf .next .swc node_modules/.cache && npm install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@hookform/resolvers": "^3.3.2",
    "@next/bundle-analyzer": "^14.0.4",
    "@reduxjs/toolkit": "^2.0.1",
    "@tanstack/react-query": "^5.14.2",
    "@tanstack/react-query-devtools": "^5.14.2",
    "@types/react-image-gallery": "^1.2.4",
    "axios": "^1.6.2",
    "clsx": "^2.0.0",
    "compromise": "^14.10.0",
    "currency.js": "^2.0.4",
    "date-fns": "^3.0.6",
    "framer-motion": "^10.16.16",
    "idb": "^8.0.3",
    "jimp": "^0.22.10",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "lucide-react": "^0.294.0",
    "next": "^14.0.4",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.2.1",
    "pdf-lib": "^1.17.1",
    "react": "^18.2.0",
    "react-camera-pro": "^1.4.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.48.2",
